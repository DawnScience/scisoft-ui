<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            category="uk.ac.diamond.scisoft.analysis.rcp"
            class="uk.ac.diamond.scisoft.qstatmonitor.views.QStatMonitorView"
            icon="icons/qstatMonitorIcon.gif"
            id="uk.ac.diamond.scisoft.qstatmonitor.views.QStatMonitorView"
            name="QStat Monitor">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="uk.ac.diamond.scisoft.analysis.rcp.preferences.root"
            class="uk.ac.diamond.scisoft.qstatmonitor.preferences.QStatMonitorPreferencePage"
            id="uk.ac.diamond.scisoft.qstatmonitor.preferences.QStatMonitorPreferencePage"
            name="QStat Monitor">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="uk.ac.diamond.scisoft.qstatmonitor.preferences.QStatMonitorPreferenceIntializer">
      </initializer>
   </extension>

</plugin>
