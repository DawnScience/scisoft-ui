              <visibleWhen
                    checkEnabled="false">
                    <and>
                        <iterate ifEmpty="false" operator="and">
                            <instanceof value="java.io.File" />
                        </iterate>
                        <count value="(1-" />
                    </and>
              </visibleWhen>
           </command>
