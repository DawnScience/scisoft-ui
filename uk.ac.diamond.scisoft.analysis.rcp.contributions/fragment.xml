                        </iterate>
                        <count value="(1-" />
                    </and>
              </visibleWhen>
           </command>
        </menuContribution>
        <menuContribution
            locationURI="popup:uk.ac.diamond.sda.navigator.views.FileView?after=uk.ac.diamond.sda.navigator.separator2">
           <command
                 commandId="uk.ac.diamond.scisoft.analysis.rcp.CompareFilesHandler"
